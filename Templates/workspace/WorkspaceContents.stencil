<?xml version="1.0" encoding="UTF-8"?>
<Workspace
   version = "1.0">
   {% for app in apps.split(',') %}
   {% if structure == "single" %}
   <FileRef
      location = "group:Project.xcodeproj">
   </FileRef>
   {% else %}
   <FileRef
      location = "group:{{ app.strip }}/Project.xcodeproj">
   </FileRef>
   {% endif %}
   {% endfor %}
   {% if structure == "multi" %}
   <FileRef
      location = "group:Shared/Project.xcodeproj">
   </FileRef>
   {% endif %}
</Workspace>